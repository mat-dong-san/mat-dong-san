		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="mat.dong.san.member.vo.Member" alias="Member"/>
		<typeAlias type="mat.dong.san.member.vo.EstateAgent" alias="EstateAgent"/>
		<typeAlias type="mat.dong.san.product.vo.Product" alias="Product"/>
